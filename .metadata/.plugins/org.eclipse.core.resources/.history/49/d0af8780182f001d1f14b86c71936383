package com.app.raghu;

import java.util.Random;

import org.springframework.stereotype.Component;

@Component
public class Token {

	private int code;

	public Token() {
		super();
		code = Math.abs( new Random().nextInt() );
	}

	@Override
	public String toString() {
		return "Token [code=" + code + "]";
	}
	
	
	
}
