package com.app.raghu;

import java.util.Random;

//@Component("t1")
//@Scope("singleton")
//@Scope("prototype")
public class Token {

	private int code;

	public Token() {
		super();
		code = Math.abs( new Random().nextInt() );
	}

	@Override
	public String toString() {
		return "Token [code=" + code + "]";
	}
	
	
	
}
